{
    "request": {
        "type": "POST",
        "path": "/auth/signup",
        "default_error": {
            "code": 400,
            "message": "Error signup."
        },
        "param": {
            "type": "form-data",
            "data": "[string:email_username],[string:password],[string:product_id]"
        },
        "data_token": "[param:email_username]",
        "sql": [{
                "type": "insert",
                "query": "INSERT INTO usertb VALUES('[string:email_username]', '[string:password]')",
                "error": {
                    "code": 400,
                    "message": "Error signup."
                }
            }
        ]
    },
    "response": {
        "token": "[marva:token]",
        "user_id": "[sql0:user_id]",
        "email": "[sql0:email]",
        "username": "[sql0:username]",
        "first_name": "[sql0:first_name]",
        "last_name": "[sql0:last_name]",
        "profile_photo": "[sql0:profile_photo]"
    }
}